{
  "name": "cowsaynode",
  "version": "1.0.0",
  "description": "A JavaScript project",
  "main": "./dist/index.js",
  "scripts": {
    "dev:watch": "node ./node_modules/parcel-bundler/bin/cli watch ./src/index.js --target node",
    "dev:hot-reload": "node ./node_modules/nodemon/bin/nodemon --watch ./dist/ ./dist/index.js",
